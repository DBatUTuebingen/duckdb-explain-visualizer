  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "bootstrap": "^5.3.2",
    "clarinet": "^0.12.5",
    "d3": "^7.8.2",
    "d3-flextree": "^2.1.2",
    "emitter": "^0.0.5",
    "highlight.js": "^11.7.0",
    "humanize-duration": "^3.28.0",
    "lodash": "^4.17.21",
    "sass": "^1.58.0",
    "splitpanes": "^4.0.0",
    "vue": "^3.2.45",
