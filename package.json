{
  "name": "duckdb-explain-visualizer",
  "version": "1.1.4",
  "homepage": "https://github.com/DBatUTuebingen/duckdb-explain-visualizer",
  "license": "PostgreSQL license",
  "files": [
    "dist"
  ],
  "main": "./dist/duckdb-explain-visualizer.umd.js",
  "module": "./dist/duckdb-explain-visualizer.mjs",
  "exports": {
    ".": {
      "import": "./dist/duckdb-explain-visualizer.mjs",
      "require": "./dist/duckdb-explain-visualizer.umd.js"
    },
    "./dist/duckdb-explain-visualizer.css": "./dist/duckdb-explain-visualizer.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-lib": "vue-tsc --noEmit && LIB=true vite build",
    "build-lib-win": "set LIB=true && npm run build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix .gitignore",
    "prepare": "husky install",
    "test": "jest || exit 0"
